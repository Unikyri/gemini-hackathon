# ====================================
# Frontend Environment Configuration (Vite)
# ====================================
# Copy this file to .env and fill in the actual values
# DO NOT commit .env file to the repository

# IMPORTANT: 
# 1. In Vite, only variables prefixed with VITE_ are exposed to the browser.
# 2. Access them in code via: import.meta.env.VITE_VARIABLE_NAME
# 3. Never put sensitive secrets (private keys, DB pass) here.

# API Configuration
VITE_API_URL=http://localhost:8080/api/v1
VITE_API_TIMEOUT=30000

# Environment (development, production, test)
VITE_APP_ENV=development

# Google Cloud (Public IDs only if needed for client-side SDKs)
# If you only use Vertex AI from Backend, you might not need this here.
VITE_GOOGLE_CLOUD_PROJECT=your-project-id

# Feature Flags
VITE_ENABLE_AI_HINTS=true
VITE_ENABLE_ANALYTICS=false

# Debugging
VITE_DEBUG_MODE=true