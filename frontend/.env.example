# ====================================
# Frontend Environment Configuration
# ====================================
# Copy this file to .env and fill in the actual values
# DO NOT commit .env file to the repository

# IMPORTANT: In React, only variables prefixed with REACT_APP_ 
# will be embedded into the build and accessible via process.env
# Never put sensitive secrets here as they will be publicly visible

# API Configuration
REACT_APP_API_URL=http://localhost:8080
REACT_APP_API_TIMEOUT=30000

# Environment
REACT_APP_ENV=development

# Google Cloud (Public keys only - no secrets!)
REACT_APP_GOOGLE_CLOUD_PROJECT=your-project-id

# Feature Flags (optional)
REACT_APP_ENABLE_AI_HINTS=true
REACT_APP_ENABLE_ANALYTICS=false

# Development Settings
REACT_APP_DEBUG_MODE=true
